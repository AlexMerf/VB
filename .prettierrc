{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/assets/(.*)$",
		"^@/components/(.*)$",
		"^@/ui/(.*)$",
		"^@/screens/(.*)$",
		"^@/hooks/(.*)$",
		"^@/store/(.*)$",
		"^@/styles/(.*)$",
		"^@/types/(.*)$",
		"^@/data/(.*)$",
		"^../(.*)",
		"^./(.*)",
		"(.scss)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
